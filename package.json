{
  "name": "starter-app",
  "version": "1.0.0",
  "description": "microservice starter",
  "main": "index.js",
  "scripts": {
    "start": "./docs.sh && babel-node index.js",
    "dev": "nodemon start --exec yarn -e js,yml,json -i swagger.json -i public",
    "build": "babel index.js -d dist",
    "docs": "./docs.sh",
    "clean:modules": "rm -rf node_modules",
    "clean:install": "yarn clean:modules && yarn clean && yarn install",
    "test": "ava -v",
    "test:watch": "ava -v --watch",
    "test:swagger": "babel-node swagger-test.js",
    "docker:dev": "docker-compose up",
    "lint": "eslint ./index.js"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@blended/starter": "^1.0.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-node7": "^1.5.0",
    "debug": "^2.6.6",
    "handlebars": "^4.0.8",
    "jschema-deref": "zanona/jschema-deref",
    "jsonwebtoken": "^7.4.0",
    "lodash": "^4.17.4",
    "pluralize": "^4.0.0",
    "request": "^2.81.0",
    "request-promise-native": "^1.0.3",
    "sequelize": "^3.30.4",
    "spectacle-docs": "^0.8.2",
    "sqlite3": "^3.1.8",
    "swagger2-koa": "^0.0.36"
  },
  "devDependencies": {
    "amanda": "^0.5.1",
    "ava": "^0.19.1",
    "babel-cli": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-syntax-async-functions": "^6.13.0",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-register": "^6.23.0",
    "babel-runtime": "^6.23.0",
    "eslint": "^3.19.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-node": "^4.2.2",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "nodemon": "^1.11.0",
    "superagent": "^3.5.2",
    "sinon": "^2.1.0"
  },
  "ava": {
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  }
}
